{
  "manifest_version": 2,
  "name": "ChessHelper - Improve Chess.com Experience",
  "description": "Chess.com extension that allows you to set up new themes, pieces, boards, fonts, remove annoying notifications, popups, and more!",
  "version": "1.0.1",
  "icons": {
    "128": "assets/icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "assets/icons/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.chess.com/*",
        "https://www.chess.com/",
        "https://chess.com/*",
        "https://chess.com/"
      ],
      "js": [
        "assets/js/content.js"
      ],
      "css": [
        "assets/graphs/main.css"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "assets/js/background.js"
    ],
    "persistent": false
  },
